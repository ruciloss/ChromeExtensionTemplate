{
	"manifest_version": 3,
	"name": "Chrome Extension Template",
	"description": "Created by Ruciloss | https://ruciloss.github.io",
	"version": "1.0.0",
	"permissions": [
		"tabs",
		"webNavigation",
		"activeTab",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
		"matches": [
			"<all_urls>"
		],
		"js": ["content.js"]
		}
	],
	"author": "Ruciloss",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
